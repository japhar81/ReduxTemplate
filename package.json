{
  "name": "reduxtemplate",
  "version": "1.0.0",
  "scripts": {
    "client:lint": "./node_modules/.bin/tslint --project ./src/tsconfig.json --config ./src/tslint.json 'src/client/**/*.ts?(x)'",
    "client:test": "./node_modules/.bin/cross-env NODE_ENV=test ./node_modules/.bin/jest --config ./test/client.json --no-cache",
    "client:test:watch": "./node_modules/.bin/cross-env NODE_ENV=test ./node_modules/.bin/jest --config ./test/client.json --watchAll",
    "client:dist": "./node_modules/.bin/cross-env NODE_ENV=prod ./node_modules/.bin/webpack --config ./webpack/client.js",
    "client:docs": "./node_modules/.bin/typedoc --exclude \"**/*.spec.*\" --out ./docs/client --module commonjs --target es6 --jsx --excludePrivate --experimentalDecorators --excludeExternals --ignoreCompilerErrors --allowJs ./src/client",
    "server:lint": "./node_modules/.bin/tslint --project ./src/tsconfig.json --config ./src/tslint.json 'src/server/**/*.ts?(x)'",
    "server:test": "./node_modules/.bin/cross-env NODE_ENV=test ./node_modules/.bin/jest --config ./test/server.json --no-cache",
    "server:test:watch": "./node_modules/.bin/cross-env NODE_ENV=test ./node_modules/.bin/jest --config ./test/server.json --watchAll",
    "server:dist": "./node_modules/.bin/cross-env NODE_ENV=prod ./node_modules/.bin/webpack --config ./webpack/server.js",
    "server:docs": "./node_modules/.bin/typedoc --exclude \"**/*.spec.*\" --out ./docs/server --module commonjs --target es6 --jsx --excludePrivate --experimentalDecorators --excludeExternals --ignoreCompilerErrors --allowJs ./src/server",
    "docs": "./node_modules/.bin/typedoc --exclude \"**/*.spec.*\" --out ./docs/combined --module commonjs --target es6 --jsx --excludePrivate --experimentalDecorators --excludeExternals --ignoreCompilerErrors --allowJs ./src",
    "prestart": "rm -rf dist && rm -rf docs",
    "start": "./node_modules/.bin/cross-env NODE_ENV=dev ./node_modules/.bin/webpack --config ./webpack/server.js --watch"
  },
  "dependencies": {
    "cross-env": "^5.1.1",
    "express": "^4.16.2",
    "express-winston": "^2.4.0",
    "isomorphic-fetch": "^2.2.1",
    "react": "^16.1.0",
    "react-dom": "^16.1.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "source-map-support": "^0.5.0",
    "winston": "^2.4.0"
  },
  "devDependencies": {
    "@types/express": "^4.0.39",
    "@types/isomorphic-fetch": "^0.0.34",
    "@types/jest": "^21.1.6",
    "@types/react": "^16.0.22",
    "@types/react-dom": "^16.0.3",
    "@types/react-hot-loader": "^3.0.5",
    "@types/react-redux": "^5.0.12",
    "@types/redux": "^3.6.0",
    "@types/redux-thunk": "^2.1.0",
    "@types/source-map-support": "^0.4.0",
    "@types/supertest": "^2.0.3",
    "@types/webpack": "^3.8.1",
    "@types/webpack-dev-middleware": "^1.9.4",
    "@types/webpack-hot-middleware": "^2.16.1",
    "@types/winston": "^2.3.7",
    "fork-ts-checker-webpack-plugin": "^0.2.8",
    "happypack": "^4.0.0",
    "jest": "^21.2.1",
    "noop-webpack-plugin": "^1.0.1",
    "react-hot-loader": "^3.1.2",
    "supertest": "^3.0.0",
    "ts-jest": "^21.2.1",
    "ts-loader": "^3.1.1",
    "tslint": "^5.8.0",
    "typedoc": "^0.9.0",
    "typescript": "^2.6.1",
    "webpack": "^3.8.1",
    "webpack-dev-middleware": "^1.11.0",
    "webpack-hot-middleware": "^2.20.0",
    "webpack-node-externals": "^1.6.0",
    "webpack-shell-plugin": "^0.5.0"
  }
}
